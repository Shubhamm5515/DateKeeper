VITE_API_URL=http://localhost:8000

# Production Backend URL (Render)
# VITE_API_URL=https://datekeeper-1.onrender.com

# Google OAuth Client ID
# Get from: https://console.cloud.google.com/apis/credentials
# Must match the Client ID in backend/.env
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Razorpay Payment Gateway
# Get from: https://dashboard.razorpay.com/app/keys
# Test mode key (for development)
VITE_RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxx

# Razorpay Plan IDs (create in dashboard)
# Must match the Plan IDs in backend/.env
VITE_RAZORPAY_PLAN_PRO=plan_xxxxxxxxxxxxx
VITE_RAZORPAY_PLAN_BUSINESS=plan_xxxxxxxxxxxxx
